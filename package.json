{
  "name": "scriptable_prusaconnectlocal",
  "version": "1.0.0",
  "description": "Widget to show 3D printer details supported by Prusa Connect local API",
  "main": "src/main.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts --environment file_path:./src/main.ts --configPlugin @rollup/plugin-typescript",
    "update_api": "npx openapi-typescript https://raw.githubusercontent.com/prusa3d/Prusa-Link-Web/master/spec/openapi.yaml --output ./src/PrusaLinkAPI.d.ts"
  },
  "author": "Filip Sádovský (sadovsf@gmail.com)",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.11",
    "@types/scriptable-ios": "^1.7.7",
    "openapi-typescript": "^7.1.0",
    "rollup": "^4.19.0",
    "scriptable-utils": "^2.2.3",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "crypto-es": "^2.1.0"
  }
}
